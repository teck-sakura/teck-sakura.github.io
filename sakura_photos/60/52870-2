<script>
$(document).ready(function () {

  if ($("#js-menu_loign").length > 0) {
    var retries2 = 0; // 現在のリトライ回数
    var retryTime2 = 1000; // リトライの間隔 ミリ秒
    var defer2 = $.Deferred();
    $.ajax({
      type: 'get',
      dataType: 'JSON',
      url: "/s/s46/api/page/message",
      data: { cd: ["menu_loign"] },
      success: function (data) {
        var section_fanclub_top_Obj = $("#js-menu_loign");
        section_fanclub_top_Obj.html(data[0].body);
        //console.log("true2");
        defer2.resolve;
      },
      error: function (jqXHR, textStatus, errorThrown) {
        // Error
        //console.log("Error2");

        if(retries2 < maxRetries){
            var _this = this;
            retryTime2 = retryTime2 * 2; // リトライを繰り返すごとにリトライ間隔を延長します
            _this.timeout = retryTime2;
            //console.warn("Connection Faild. Retry After " + retryTime2 + " (ms)");

            retries2++;
            setTimeout(function(){ $.ajax(_this) }, retryTime2); // retry begin
          } else {
            //console.warn("Connection Timeout.");
            //console.warn(jqXHR.status + " " + jqXHR.statusText);

            defer2.reject(); // retry end
          }
        }
    });
    defer2.promise();
    //console.log("defer2.promise");
  }
});
</script>

