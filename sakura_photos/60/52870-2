<script>
function doSubmit(nam,val) {
  console.log("nam",nam)
  console.log("val",val)
  console.log("document.pageForm.elements[nam]",document.pageForm.elements[nam])
  if (document.pageForm.elements[nam]) {
    console.log("test");
    document.pageForm.elements[nam].value = val;
  } else {
    console.log("test2");
    document.pageForm.elements[0].name = nam;
    document.pageForm.elements[0].value = val;
  }
  document.pageForm.submit();
}

function NextMonth(){
  objDate = new Date('//01');
  y = objDate.getFullYear();
  m = objDate.getMonth()+1;
  if (m==12) { m= 1;y++; }else{m++;}
 ym = y + fillZero(m);
 return ym;
}

function BackMonth(){
  objDate = new Date('//01');
  y = objDate.getFullYear();
  m = objDate.getMonth()-1;
  if (m< 0) { m= 12;y--; }else{m++;}
 ym = y + fillZero(m);
 return ym;
}

function fillZero(numb) {
  numb += "";
  if (numb.length === 1) {
    numb= "0" + numb;
  }
 return numb;     
}

$(document).ready(function() {
  objDate = new Date('//01');
  y = objDate.getFullYear();
  m = objDate.getMonth()+1;
  if (m==12) { m= 1;y++; }else{m++;}
 ym = y + fillZero(m);
 if(202311 < ym){
  $('.next-month').empty();
  $('.next-month').addClass("empty");
   }
});
    $('.js-member-filter .check-all').on('click', function(event) {
      allCheck();
    });
    $('.js-member-filter .check-all_not').on('click', function(event) {
      allRemoveCheck();
    });

  function allCheck() {
    $('.js-member-filter .member-box li:not(.hidden) input[type="checkbox"]').prop('checked', true).trigger('change');
  }

  function allRemoveCheck() {
    $('.js-member-filter .member-box li input[type="checkbox"]').prop('checked', false).trigger('change');
  }
    $('.js-member-filter .member-box .box-img').on('click', function(event) {
      var id =  $(event.target).closest('li').attr('data-member');
      var checked = $("#"+id+"").prop('checked');
      if (!checked) {
        $("#"+id+"").prop('checked',true);
        $(event.target).closest('li').addClass('checked');
       
      }
      else {
        $(event.target).closest('li').removeClass('checked');
        $("#"+id+"").prop('checked',false);
        $("#pageForm input[name='list[]'][type='hidden'][value='"+id+"']").remove();
      }
    });

    $('.js-member-filter .member-box input[type="checkbox"]').on('change', function(event) {
      var checked = $(event.target).prop('checked');
      if (checked) {
        $(event.target).closest('li').addClass('checked');
      }
      else {
        $(event.target).closest('li').removeClass('checked');
        var id =  $(event.target).val();
        $("#pageForm input[name='list[]'][type='hidden'][value='"+id+"']").remove();
      }
    });
</script>

