<script>
window.onload = function() {
setTimeout(function() {
const agent = window.navigator.userAgent;
console.log(agent);
if (agent.indexOf("APPWebview") != -1) {
document.querySelector("header").style.display = "none";
document.querySelector("footer").style.display = "none";
const navtop = document.querySelector('#nav_toggle');
const thummenu = document.querySelector('.fc_fixed_menu');
if( navtop != null){navtop.style.display = "none!important";}
if( thummenu != null){
thummenu.removeAttribute('style');
thummenu.style.setProperty('display', 'none', 'important');
}
}else{
const navtop = document.querySelector('#nav_toggle');
if( navtop != null){
        navtop.style.opacity = "0";
        navtop.style.display = "block";
        navtop.style.transition = "opacity 1s";
        setTimeout(function() {
          navtop.style.opacity = "1";
        }, 1000);
}
}
}, 10); 
}

</script>
